\PYG{esc}{\textbf{SUBROUTINE} RemoveTop()}
    \PYG{esc}{TempSwap \leftarrow HeapList[-1]}
    \PYG{esc}{HeapList[-1] \leftarrow HeapList[0]}
    \PYG{esc}{HeapList[0] \leftarrow TempSwap}
